        </td>
        <td>
            @Html.DisplayFor(modelItem => item.HandleResult)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsAccept)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.BxId }) |
            @Html.ActionLink("Details", "Details", new { id=item.BxId }) |
